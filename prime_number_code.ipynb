   "source": [
    "n=int(input(\"enter the number:\"))\n",
    "count=0\n",
    "if(n<=1):\n",
    "    print(\"number equals to or less than 1 cant be prime\")\n",
    "else:\n",
    "    for i in range(2,n):\n",
    "        if(n%i==0):\n",
    "            count=1\n",
    "            break\n",
    "    if(count==0):\n",
    "        print(\"prime\")\n",
    "    else:\n",
    "        print(\"not a prime\")"
   ]
